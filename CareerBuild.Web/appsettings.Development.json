{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft": "Information",
				"System": "Information",
				"Microsoft.AspNetCore": "Information"
			}
		},
		"Using": ["Serilog.Sinks.Console"],
		"Enrich": [
			"FromLogContext",
			"WithMachineName",
			"WithProcessId",
			"WithThreadId"
		],
		"WriteTo": [
			{
				"Name": "Console"
			}
		]
	}
}
